<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns="http://dozer.sourceforge.net"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">

    <mapping>
        <class-a>nl.quintor.studybits.student.entities.ClaimEntity</class-a>
        <class-b>nl.quintor.studybits.student.models.ClaimModel</class-b>
        <field type="one-way">
            <a>student.userName</a>
            <b>studentUserName</b>
        </field>
        <field type="one-way">
            <a>student.originUniversity.name</a>
            <b>issuingUniversityName</b>
        </field>
        <field type="one-way">
            <a>schemaKey.name</a>
            <b>schemaKeyName</b>
        </field>
    </mapping>
</mappings>